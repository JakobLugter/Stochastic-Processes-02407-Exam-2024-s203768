    "from scipy.stats import expon\n",
    "import numpy as np\n",
    "service_rate = 1/8\n",
    "time = 24\n",
    "\n",
    "# P service takes longer than 24 hours\n",
    "p = 1- expon.cdf(scale = 1/service_rate, x = time)\n",
    "print(p)\n",
    "\n",
    "# Alternativey directily from the definition\n",
    "np.exp(- service_rate * time)"
   ]
